<header class="dfe-header" role="banner">
  <a href="#mainContent" class="govuk-skip-link" data-module="govuk-skip-link">Skip to main content</a>
  <div class="dfe-width-container dfe-header__container">
    <div class="dfe-header__logo">
      <a class="dfe-header__link dfe-header__link--service " href="/">
        <img src="~/dist/images/dfe-logo.png" class="dfe-logo" alt="Department for Education">
        <img src="~/dist/images/dfe-logo-alt.png" class="dfe-logo-hover" alt="Department for Education">
        <span class="dfe-header__service-name">
          @Constants.ServiceName
        </span>
      </a>
    </div>
  </div>
</header>